{
  "name": "server-master-ts",
  "version": "0.1.0",
  "description": "Template for setting up Node server using Typescript, Express, nodemon",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc -p .",
    "format": "prettier . --write",
    "eslint:fix": "eslint --fix",
    "prepare": "husky install"
  },
  "keywords": [
    "node",
    "typescript",
    "express",
    "nodemon",
    "jest"
  ],
  "author": "Darby Ross",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
